DROP TABLE IF EXISTS Mascota;
DROP TABLE IF EXISTS Especie;
DROP TABLE IF EXISTS Vacuna;
DROP TABLE IF EXISTS Cliente;


-- Tabla Cliente (uno a muchos con Mascota)
CREATE TABLE Cliente (
    dni VARCHAR(20) PRIMARY KEY,
    nombre VARCHAR(100),
    apellidos VARCHAR(100)
);

-- Tabla Animal (uno a uno con Mascota)
CREATE TABLE Especie (
    especie VARCHAR(50) PRIMARY KEY,
    vida INT -- Esperanza de vida en años
);

-- Tabla Mascota (muchos a uno con Cliente, uno a uno con Especie, muchos a muchos con Vacuna)
CREATE TABLE Mascota (
    chip VARCHAR(20) PRIMARY KEY,
	nombre VARCHAR(100),
    cliente VARCHAR(20),
	especie VARCHAR(50),
    FOREIGN KEY (cliente) REFERENCES Cliente(dni),
	FOREIGN KEY (especie) REFERENCES Especie(especie)
);

-- Tabla Vacuna (muchos a muchos con Mascota)
CREATE TABLE Vacuna (
    vacuna VARCHAR(100),
    precio DECIMAL(10, 2),
    PRIMARY KEY (vacuna)
);

-- Tabla intermedia para la relación muchos a muchos entre Mascota y Vacuna
CREATE TABLE Mascota_Vacuna (
    chip VARCHAR(20),
    vacuna VARCHAR(100),
    PRIMARY KEY (chip, vacuna),
    FOREIGN KEY (chip) REFERENCES Mascota(chip),
    FOREIGN KEY (vacuna) REFERENCES Vacuna(vacuna)
);

